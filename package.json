{
  "name": "nydus-client",
  "version": "0.2.6",
  "description": "WebSocket client library for the nydus protocol, a simple RPC/PubSub protocol.",
  "main": "lib/index.js",
  "scripts": {
    "autotest": "mocha --watch -R spec --compilers js:babel/register --recursive test/",
    "build": "babel index.js --out-dir lib/",
    "clean": "rimraf lib",
    "lint": "eslint --ext .js ./",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build",
    "test": "mocha -R spec --compilers js:babel/register --recursive test/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tec27/nydus-client"
  },
  "keywords": [
    "websocket",
    "rpc",
    "pubsub",
    "websockets",
    "nydus"
  ],
  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tec27/nydus-client/issues"
  },
  "homepage": "https://github.com/tec27/nydus-client",
  "dependencies": {
    "backo": "^1.1.0",
    "cuid": "^1.3.8",
    "engine.io-client": "^1.5.4",
    "immutable": "^3.7.5",
    "nydus-protocol": "^0.3.1",
    "ruta3": "^2.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "eslint": "^1.6.0",
    "eslint-plugin-babel": "^2.1.1",
    "mocha": "^2.3.3",
    "nydus": "^0.3.0",
    "rimraf": "^2.4.3"
  }
}
